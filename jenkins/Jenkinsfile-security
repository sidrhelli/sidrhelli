pipeline {

    agent any
    
    stages {

        stage('Initialize') {

            steps {
                echo "Preparing build..."
            }
        }
        stage('OWASP Dependency Check') {

            steps {
                sh 'chmod +x jenkins/owasp-dependency-check.sh'
                sh 'bash jenkins/owasp-dependency-check.sh JENKINS_HOME'
            }
        }
    }
}